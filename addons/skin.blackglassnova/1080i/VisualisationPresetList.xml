<?xml version="1.0" encoding="utf-8"?>
<window>
    <defaultcontrol always="true">5</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <include>DialogOpenClose</include>
    <controls>
        <include>DimBG</include>
        <control type="group">
            <include condition="Skin.HasSetting(BlackGlassMode)+!Skin.HasSetting(DisableSlidingAnimations)">OpenCloseSlideRight</include>
            <posx>500</posx>
            <posy>202</posy>
            <control type="button">
                <visible>Skin.HasSetting(WindowClosingIcon)</visible>
                <posx>870</posx>
                <posy>-4</posy>
                <height>50</height>
                <width>50</width>
                <texturenofocus>topbar/topbar_exit_nofocus.png</texturenofocus>
                <texturefocus>topbar/topbar_exit_focus.png</texturefocus>
                <onclick>back</onclick>
                <label>-</label>
            </control>
            <control type="image">
                <posx>0</posx>
                <posy>0</posy>
                <width>920</width>
                <height>675</height>
                <texture border="90">common/bg_dialog.png</texture>
            </control>
            <control type="label" id="2">
                <posx>90</posx>
                <posy>55</posy>
                <width>740</width>
                <height>100</height>
                <align>center</align>
                <font>InfoTitleFont</font>
                <textcolor>MainColor</textcolor>
                <label>-</label>
            </control>
            <control type="image">
                <posx>45</posx>
                <posy>75</posy>
                <width>830</width>
                <height>79</height>
                <texture>list/bg_facade_nofocus_top.png</texture>
            </control>
            <control type="fixedlist" id="5">
                <description>Preset List</description>
                <posx>45</posx>
                <posy>154</posy>
                <width>830</width>
                <height>438</height>
                <itemgap>0</itemgap>
                <orientation>vertical</orientation>
                <onleft>noop</onleft>
                <onright>60</onright>
                <pagecontrol>60</pagecontrol>
                <focusposition>3</focusposition>
                <movement>3</movement>
                <onup>noop</onup>
                <ondown>noop</ondown>
                <itemlayout width="830" height="73">
                    <control type="image">
                        <posx>0</posx>
                        <posy>0</posy>
                        <height>73</height>
                        <width>830</width>
                        <texture border="3">list/bg_facade_nofocus.png</texture>
                    </control>
                    <control type="label">
                        <posx>TextMargin</posx>
                        <height>71</height>
                        <width>740</width>
                        <font>FacadeListMainFont</font>
                        <textcolor>ListNoFocusColor</textcolor>
                        <info>ListItem.Label</info>
                        <scroll>false</scroll>
                    </control>
                </itemlayout>
                <focusedlayout width="830" height="73">
                    <control type="image">
                        <posx>0</posx>
                        <posy>0</posy>
                        <height>73</height>
                        <width>830</width>
                        <include>FacadeFocus</include>
                    </control>
                    <control type="label">
                        <posx>TextMargin</posx>
                        <height>71</height>
                        <width>740</width>
                        <font>FacadeListMainFont</font>
                        <textcolor>ListFocusColor</textcolor>
                        <info>ListItem.Label</info>
                    </control>
                </focusedlayout>
            </control>
            <control type="scrollbar" id="60">
                <posx>844</posx>
                <posy>154</posy>
                <height>438</height>
                <include>VerticalScroller</include>
                <onleft>5</onleft>
                <onright>noop</onright>
            </control>
        </control>
    </controls>
</window>
